language: de_DE
tag: user.multiple_cursors
-
aktiviere mehrere cursor: user.multi_cursor_enable()
cursor stop: user.multi_cursor_disable()
cursor hoch: user.multi_cursor_add_above()
cursor runter: user.multi_cursor_add_below()
weniger cursor: user.multi_cursor_select_fewer_occurrences()
zusätzlicher cursor: user.multi_cursor_select_more_occurrences()
cursor überspringen: user.multi_cursor_skip_occurrence()
alle cursor: user.multi_cursor_select_all_occurrences()
cursor zeilen: user.multi_cursor_add_to_line_ends()
